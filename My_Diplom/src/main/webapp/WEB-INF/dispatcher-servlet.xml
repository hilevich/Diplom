<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:mvc="http://www.springframework.org/schema/mvc"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans     
        http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
        http://www.springframework.org/schema/mvc 
        http://www.springframework.org/schema/mvc/spring-mvc-3.2.xsd
        http://www.springframework.org/schema/context 
        http://www.springframework.org/schema/context/spring-context-3.2.xsd">
 
	<context:component-scan base-package="com.google" />
	
	 <bean name ="/home.htm" class="com.google.controller.HomePage"></bean>
	 
	 <bean name ="/contact_home.htm" class="com.google.controller.Contact_home"></bean>
	 
	 <bean name ="/contact_office.htm" class="com.google.controller.Contact_Office"></bean>
	 
	 <bean name ="/login.htm" class="com.google.controller.Login"></bean>
	 
	 <bean name ="/chekers.htm" class="com.google.controller.ChekersContcoller"></bean>
	 
	 <bean name ="/tetris.htm" class="com.google.controller.TetrisController"></bean>
	 
	 <bean name ="/seabatlle.htm" class="com.google.controller.SeabatlleController"></bean>
	 
	 <bean name ="/bubbles.htm" class="com.google.controller.BubblesControler"></bean>
	
	 <bean name ="/logOut.htm" class="com.google.controller.LogOutServlet"></bean>
	 
	
	<mvc:resources location="/resources/**" mapping="/assets/**"/>
 
</beans>